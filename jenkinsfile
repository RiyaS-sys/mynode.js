pipeline {
    agent any

    environment {
        GIT_CREDENTIALS = 'My_project' // Replace with the ID of your Git credentials in Jenkins
    }

    stages {
        stage('Clone Repo') {
            steps {
                // Clone the repository, specify the correct branch and credentials
                git branch: 'main', credentialsId: "${GIT_CREDENTIALS}", url: 'https://github.com/RiyaS-sys/mynode.js'
            }
        }

        stage('Install Dependencies') {
            steps {
                // Installing dependencies using npm
                echo 'Installing dependencies...'
                bat 'npm install'
            }
        }

        stage('Test') {
            steps {
                // Running tests using npm test (if any)
                echo 'Running tests...'
                bat 'npm test'
            }
        }

        stage('Deploy') {
            steps {
                // Add your deployment commands here
                echo 'Deploying application...'
                bat 'echo Deployment process will be executed here.'
            }
        }
    }
}










